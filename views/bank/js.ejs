<script src="https://cdn.datatables.net/v/bs5/dt-1.12.1/datatables.min.js"></script>
<script src="/template/assets/js/pages/datatables.js"></script>
  <script src="/template/assets/extensions/toastify-js/src/toastify.js"></script>
  <script src="/template/assets/js/pages/toastify.js"></script>
<script src="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/js/select2.min.js"></script>

<script>
  $(document).ready(function () {
    $(".choices").select2({
      theme: "classic",
      placeholder: "Pilih Item",
      allowClear: true,
    });
    $(".choices-nominal").select2({
      theme: "classic",
      placeholder: "Pilih Item",
      allowClear: true,
    });

    $(".choices").trigger("change");
    $(".choices-nominal").trigger("change");

    function readURL(input) {
      if (input.files && input.files[0]) {
        const reader = new FileReader();
        reader.onload = function (e) {
          $("#preview").attr("src", e.target.result);
          $("#preview").attr("width", "20%");
        };
        reader.readAsDataURL(input.files[0]);
      }
    }

    $("#image").change(function () {
      readURL(this);
    });
  });
</script>
<%if( typeof alert != 'undefined' && alert.message != ""){ %>
<script>
  Toastify({
    text: "<%= alert.message %>",
    duration: 3000,
    close: true,
    gravity: "top",
    position: "center",
    className: "bg-<%= alert.status %>",
    backgroundColor: "#40a944",
  }).showToast();
</script>
<%}%>
